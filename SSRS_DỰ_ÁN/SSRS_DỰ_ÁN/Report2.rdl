  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT TOP (7) company.company_name, COUNT(*) AS job_count
FROM   jobs INNER JOIN
             company ON jobs.company_id = company.company_id
GROUP BY company.company_name
ORDER BY job_count DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="company_name">
          <DataField>company_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="job_count">
          <DataField>job_count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
